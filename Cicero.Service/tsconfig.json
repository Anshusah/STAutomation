{
    "compilerOptions": {
        "noImplicitAny": true,
        "noEmitOnError": true,
        "removeComments": false,
        "sourceMap": true,
        "target": "es2015",
        "lib": [ "es5", "dom", "es2015.promise" ],
        "noImplicitThis": true,
        "skipLibCheck": true
    },
  "exclude": [
    "node_modules",
    "wwwroot"
  ]
}
