@using Cicero.Service.Models.Core.Elements
@model Cicero.Service.Models.Core.Elements.SelectBox
@using Cicero.Service.Services
@using Cicero.Service.Models
@inject ICommonService commonService
@inject ISynchronizeService SynchronizeService
@inject IQueueService QueueService
@inject IFormBuilderService FormBuilderService
@{
    SelectBox sb = Model as SelectBox;
    var xx = sb;
    int formId = Convert.ToInt32(sb.Data);
    List<SelectListItem> dbTables = SynchronizeService.getSyncSourceTable();
    string loggeduser = commonService.getLoggedInUserId();
    string roleId = commonService.GetRoleIdByUserId(loggeduser);
    List<SelectListItem> stateList = QueueService.GetStateSelectListByFormId(formId, roleId);
    List<SelectListItem> formElements = FormBuilderService.GetFormFields(formId, true);
    List<SelectListItem> formAllElements = FormBuilderService.GetFormFields(formId, true, true);
}
<div class="modal-header flex-column">
    <h5 class="modal-title">SelectBox Setting</h5>
    <p class="mb-0">This settings helps you to customize SelectBox.</p>
</div>

<div class="modal-body">
    <div class="status"></div>
    <ul class="fb-card">
        <li class="active">
            <header>Name</header>
            <section>
                <div class="form-row">
                    <div class="form-group col-lg-6">
                        <label for="rgName">Element Name</label>
                        <input type="text" class="form-control" id="ElementName" @sb.GetName("Name") value="@sb.Name" data-name="elmName" />
                        <small class="form-text text-muted">Input a Unique Name for this Element.</small>
                        <span class="text-danger field-validation-valid" data-valmsg-for="ElementName_@sb.ElementId" data-valmsg-replace="true"></span>
                    </div>
                </div>
            </section>
        </li>

        <li>
            <header>Frontend Setting</header>
            <section>
                <div class="form-row">
                    <div class="form-group col-lg-4">
                        <label for="sbFrontendId">Id</label>
                        <input type="text" class="form-control" id="sbFrontendId" @sb.GetName("FrontendId") value="@sb.FrontendId">
                    </div>
                    <div class="form-group col-lg-4">
                        <label for="sbFrontendClass">Class</label>
                        <input type="text" class="form-control" id="sbFrontendClass" @sb.GetName("FrontendClass") value="@sb.FrontendClass">
                    </div>
                    <div class="form-group col-lg-4">
                        <label for="sbFrontendLabel">Label</label>
                        <input type="text" class="form-control" id="sbFrontendLabel" @sb.GetName("FrontendLabel") value="@sb.FrontendLabel">
                    </div>
                </div>
                <div class="switch-control">
                    <label for="sbFrontendVisible" class="switch">
                        <input class="switch__input" id="sbFrontendVisible" @sb.GetName("FrontendVisible") type="checkbox" value="true" @sb.GetChecked(sb.FrontendVisible)>
                        <span class="switch__state"></span>
                        <span class="switch__label">Default visible</span>
                    </label>

                </div>
                <div class="switch-control">
                    <label for="sbVisibleinFooter" class="switch">
                        <input class="switch__input" id="sbVisibleinFooter" @sb.GetName("VisibleinFooter") type="checkbox" value="true" @sb.GetChecked(sb.VisibleinFooter)>
                        <span class="switch__state"></span>
                        <span class="switch__label">Visible In Footer</span>
                    </label>
                </div>
            </section>
        </li>
        <li>
            <header>Backend Setting</header>
            <section>
                <div class="form-row">
                    <div class="form-group col-lg-4">
                        <label for="sbBackendId">Id</label>
                        <input type="text" class="form-control" id="sbBackendId" @sb.GetName("BackendId") value="@sb.BackendId">
                    </div>
                    <div class="form-group col-lg-4">
                        <label for="sbBackendClass">Class</label>
                        <input type="text" class="form-control" id="sbBackendClass" @sb.GetName("BackendClass") value="@sb.BackendClass">
                    </div>
                    <div class="form-group col-lg-4">
                        <label for="sbBackendLabel">Label</label>
                        <input type="text" class="form-control" id="sbBackendLabel" @sb.GetName("BackendLabel") value="@sb.BackendLabel">
                    </div>
                </div>
                <div class="switch-control">
                    <label for="sbBackendVisible" class="switch">
                        <input class="switch__input" id="sbBackendVisible" @sb.GetName("BackendVisible") type="checkbox" value="true" @sb.GetChecked(sb.BackendVisible)>
                        <span class="switch__state"></span>
                        <span class="switch__label">Default visible</span>
                    </label>
                </div>
            </section>
        </li>

        <li>
            <header>
                <span>Select Option </span>
            </header>
            <section>
                <div class="form-row">
                    <div class="col-lg-12">
                        <div class="switch-control">
                            <label for="UseFromDatabase" class="switch">
                                <input class="switch__input" id="UseFromDatabase" @sb.GetName("UseFromDatabase") type="checkbox" value="true" @sb.GetChecked(sb.UseFromDatabase)>
                                <span class="switch__state"></span>
                                <span class="switch__label">Take Options From DB</span>
                            </label>
                        </div>
                    </div>
                </div>
                <hr />
                <div id="exTab2" class="container p-0">
                    <ul class="nav nav-tabs">
                        <li class="@(sb.UseFromDatabase == true ? "" : "active")">
                            <a href="#manualOptions" data-toggle="tab" class="nav-link @(sb.UseFromDatabase == true ? "" : "active")">Manual Option Input</a>
                        </li>
                        <li class="@(sb.UseFromDatabase == true ? "active" : "")">
                            <a href="#dbOptions" data-toggle="tab" class="nav-link @(sb.UseFromDatabase == true ? "active" : "")">Link With Database</a>
                        </li>
                    </ul>

                    <div class="tab-content">
                        <div class="tab-pane pt-4 pb-3 @(sb.UseFromDatabase == true ? "" : "active")" id="manualOptions">
                            <div class="select-option-head">
                                <div class="px-4">
                                    <div class="form-row">
                                        <div class="col-lg-3">
                                            <label for="">Value Field</label>
                                        </div>
                                        <div class="col-lg-2">
                                            <label for="">Text Field</label>
                                        </div>
                                        <div class="col-lg-2">
                                            <label for="">Icon URL</label>
                                        </div>
                                        <div class="col-lg-2">
                                            <label for="">Default</label>
                                        </div>
                                        <div class="col-lg-2">
                                            <label for="">Toggle</label>
                                        </div>
                                    </div>

                                </div>
                            </div>
                            <div class="validation_rule_wrapper">
                                @{
                                    int index1 = 0;
                                    if (sb.SelectOptions != null && sb.SelectOptions.Count > 0)
                                    {

                                        foreach (var so in sb.SelectOptions)
                                        {
                                            <div class="field-validation-wrapper">
                                                <div class="field-controller">
                                                    <a class="fc-icon icon-copy" data-action='clone-rule' title="Copy"><i class="ri-file-copy-line"></i></a>
                                                    <a class="fc-icon icon-delete" data-action='remove-rule' title="Delete"><i class="ri-delete-bin-4-line"></i></a>
                                                    <a class="fc-icon icon-timer" data-action='short-selectoption'><i class="ri-timer-2-line"></i></a>
                                                </div>
                                                <div class="form-row">
                                                    <div class="col-lg-3">
                                                        <input type="text" name="SelectOptions[@index1][Value]" data-name="SelectOptions[{0}][Value]" class="form-control" id="" placeholder="Value Field" value="@so.Value">

                                                    </div>
                                                    <div class="col-lg-2">
                                                        <input type="text" name="SelectOptions[@index1][Text]" data-name="SelectOptions[{0}][Text]" class="form-control" id="" placeholder="Text Field" value="@so.Text">

                                                    </div>
                                                    <div class="col-lg-2">
                                                        <input type="text" name="SelectOptions[@index1][IconUrl]" data-name="SelectOptions[{0}][IconUrl]" class="form-control" id="" placeholder="Icon Url" value="@so.IconUrl">

                                                    </div>
                                                    <div class="col-lg-2">
                                                        <label class="switch">
                                                            <input type="checkbox" name="SelectOptions[@index1][Selected]" class="switch__input" data-name="SelectOptions[{0}][Selected]" value="True" id="" checked="@so.Selected">
                                                            <span class="switch__state"></span>
                                                            <span class="switch__label"></span>
                                                        </label>

                                                    </div>
                                                    <div class="col-lg-2">
                                                        <label class="switch">
                                                            <input type="checkbox" name="SelectOptions[@index1][ToggleOptions]" class="switch__input" data-name="SelectOptions[{0}][ToggleOptions]" value="True" id="" checked="@so.ToggleOptions">
                                                            <span class="switch__state"></span>
                                                            <span class="switch__label"></span>
                                                        </label>

                                                    </div>

                                                </div>

                                            </div>
                                            {
                                                index1++;
                                            }
                                        }

                                    }
                                    else
                                    {
                                        <div class="field-validation-wrapper">
                                            <div class="field-controller">
                                                <a class="fc-icon icon-copy" data-action='clone-rule' title="Copy"><i class="ri-file-copy-line"></i></a>
                                                <a class="fc-icon icon-delete" data-action='remove-rule' title="Delete"><i class="ri-delete-bin-4-line"></i></a>
                                                <a class="fc-icon icon-timer" data-action='short-selectoption'><i class="ri-timer-2-line"></i></a>
                                            </div>
                                            <div class="form-row">

                                                <div class="col-lg-3">
                                                    <input type="text" name="SelectOptions[0][Value]" data-name="SelectOptions[{0}][Value]" class="form-control" id="" placeholder="Value Filed">
                                                </div>
                                                <div class="col-lg-2">
                                                    <input type="text" name="SelectOptions[0][Text]" data-name="SelectOptions[{0}][Text]" class="form-control" id="" placeholder="Text Field">

                                                </div>
                                                <div class="col-lg-2">
                                                    <input type="text" name="SelectOptions[0][IconUrl]" data-name="SelectOptions[{0}][IconUrl]" class="form-control" id="" placeholder="Icon Url">

                                                </div>
                                                <div class="col-lg-2">
                                                    <label class="switch">
                                                        <input type="checkbox" name="SelectOptions[@index1][Selected]" class="switch__input" data-name="SelectOptions[{0}][Selected]" value="True" id="">
                                                        <span class="switch__state"></span>
                                                        <span class="switch__label"></span>
                                                    </label>
                                                </div>
                                                <div class="col-lg-2">
                                                    <label class="switch">
                                                        <input type="checkbox" name="SelectOptions[@index1][ToggleOptions]" class="switch__input" data-name="SelectOptions[{0}][ToggleOptions]" value="True" id="">
                                                        <span class="switch__state"></span>
                                                        <span class="switch__label"></span>
                                                    </label>
                                                </div>

                                            </div>

                                        </div>
                                    }
                                }
                            </div>
                        </div>
                        <div class="tab-pane pt-4 pb-3 @(sb.UseFromDatabase == true ? "active" : "")" id="dbOptions">
                            @{
                                string selectedTable = string.Empty;
                            }
                            <div class="form-row">
                                <div class="form-group col-lg-3">
                                    <label for="sbSelectFromDbTable">Select Available Table</label>
                                    <select class="form-control dbTable" id="sbSelectFromDbTable" @sb.GetName("SelectFromDbTable") value="@sb.SelectFromDbTable">
                                        <option value="">Please Select</option>
                                        @foreach (var item in dbTables)
                                        {
                                            if (item.Value == sb.SelectFromDbTable)
                                            {
                                                <option value="@item.Value" selected>@item.Text</option>
                                                selectedTable = item.Value;
                                            }
                                            else
                                            {
                                                <option value="@item.Value">@item.Text</option>
                                            }

                                        }
                                    </select>
                                </div>
                                <div class="form-group col-lg-3">
                                    <label for="sbSelectFromDbFieldText">Select Available (Text)</label>
                                    <select class="form-control dbField" id="sbSelectFromDbFieldText" @sb.GetName("SelectFromDbFieldText") value="@sb.SelectFromDbFieldText">
                                        @if (selectedTable != string.Empty)
                                        {
                                            List<SelectListItem> tableFields = SynchronizeService.getSyncSourceTableColumns(selectedTable);
                                            @foreach (var item in tableFields)
                                            {
                                                if (item.Value == sb.SelectFromDbFieldText)
                                                {
                                                    <option value="@item.Value" selected>@item.Text</option>
                                                }
                                                else
                                                {
                                                    <option value="@item.Value">@item.Text</option>
                                                }
                                            }
                                        }
                                        else
                                        {
                                            <option value="">Please Select Table First</option>
                                        }
                                    </select>
                                </div>
                                <div class="form-group col-lg-3">
                                    <label for="sbSelectFromDbFieldValue">Select Field (Value)</label>
                                    <select class="form-control dbField" id="sbSelectFromDbFieldValue" @sb.GetName("SelectFromDbFieldValue") value="@sb.SelectFromDbFieldValue">
                                        @if (selectedTable != string.Empty)
                                        {
                                            List<SelectListItem> tableFields = SynchronizeService.getSyncSourceTableColumns(selectedTable);
                                            @foreach (var item in tableFields)
                                            {
                                                if (item.Value == sb.SelectFromDbFieldValue)
                                                {
                                                    <option value="@item.Value" selected>@item.Text</option>
                                                }
                                                else
                                                {
                                                    <option value="@item.Value">@item.Text</option>
                                                }
                                            }
                                        }
                                        else
                                        {
                                            <option value="">Please Select Table First</option>
                                        }
                                    </select>
                                </div>
                                <div class="form-group col-lg-3">
                                    <label for="sbSelectFromDbIconUrl">Select For (Icon)</label>
                                    <select class="form-control dbField" id="sbSelectFromDbIconUrl" @sb.GetName("SelectFromDbIconUrl") value="@sb.SelectFromDbIconUrl">
                                        @if (selectedTable != string.Empty)
                                        {
                                            List<SelectListItem> tableFields = SynchronizeService.getSyncSourceTableColumns(selectedTable);
                                            @foreach (var item in tableFields)
                                            {
                                                if (item.Value == sb.SelectFromDbIconUrl)
                                                {
                                                    <option value="@item.Value" selected>@item.Text</option>
                                                }
                                                else
                                                {
                                                    <option value="@item.Value">@item.Text</option>
                                                }
                                            }
                                        }
                                        else
                                        {
                                            <option value="">Please Select Table First</option>
                                        }
                                    </select>
                                </div>
                            </div>
                            <div class="form-row">


                                <div class="col-lg-3 form-group">
                                    <label for="sbSelectFromDbFieldSelectedOnRow">Input Default Selected</label>
                                    <input type="number" min="0" class="form-control" placeholder="Eg: 1 for 1st option" id="sbSelectFromDbFieldSelectedOnRow" @sb.GetName("SelectFromDbFieldSelectedOnRow") value="@sb.SelectFromDbFieldSelectedOnRow" />

                                </div>
                                <div class="col-lg-3 form-group">
                                    <label for="sbSelectFromDbToggleOptionsOnRow">Input Default Toggle</label>
                                    <input type="number" min="0" class="form-control" placeholder="Eg: 1 for 1st option" id="sbSelectFromDbToggleOptionsOnRow" @sb.GetName("SelectFromDbToggleOptionsOnRow") value="@sb.SelectFromDbToggleOptionsOnRow" />

                                </div>

                                <div class="form-group col-lg-3">
                                    <div class="switch-control">
                                        <label for="sbSelectFromDbIsToggleOption" class="switch">
                                            <input class="switch__input" id="sbSelectFromDbIsToggleOption" @sb.GetName("SelectFromDbIsToggleOption") type="checkbox" value="true" @sb.GetChecked(sb.SelectFromDbIsToggleOption)>
                                            <span class="switch__state"></span>
                                            <span class="switch__label">Enable Toggle</span>
                                        </label>
                                    </div>
                                </div>
                            </div>
                        </div>

                    </div>
                </div>
            </section>

        </li>

        <li>
            <header>Events Setting</header>
            <section>
                <div id="Events" class="container p-0">
                    <ul class="nav nav-tabs">
                        <li class="active">
                            <a href="#onLoad" data-toggle="tab" class="nav-link active">OnLoad Event</a>
                        </li>
                        <li>
                            <a href="#onChange" data-toggle="tab" class="nav-link">OnChange Event</a>
                        </li>
                    </ul>
                    <div class="tab-content">
                        <div class="tab-pane pt-4 pb-3 active" id="onLoad">

                            <div class="row">
                                <div class="col-lg-12 col-md-12 mb-12">
                                    <div class="target_option_wrapper">
                                        <div class="field-validation-wrapper">
                                            <div class="form-row">
                                                <div class="form-group col-md-6">
                                                    <div class="switch-control">
                                                        <label for="IsOnLoadEvent" class="switch">
                                                            <input class="switch__input" id="IsOnLoadEvent" @sb.GetName("IsOnLoadEvent") type="checkbox" value="true" @sb.GetChecked(sb.IsOnLoadEvent)>
                                                            <span class="switch__state"></span>
                                                            <span class="switch__label">Enable Workflow Event</span>
                                                        </label>
                                                    </div>
                                                </div>
                                                <div class="col-md-6">
                                                    <div class="form-group">
                                                        <button onclick="openElementEventSetting(@formId,@sb.ElementId,@Convert.ToInt16(Cicero.Service.Enums.ElementEventType.OnLoad))" type="button" class="btn btn-outline-primary form-control element-workflow">
                                                            <i class="ri-shape-line"></i> <span>Manage On Load Workflow Event</span>
                                                        </button>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>

                                    </div>
                                </div>

                            </div>



                            @*<div class="form-row">
                                    <div class="col-lg-12">
                                        <p>Choose states from the options available options to run workflow.</p>
                                    </div>
                                </div>
                                <div class="form-row">
