                                        <label for="sbStartState">Select Start State</label>
                                        <select class="form-control startState" id="sbStartState" name="OnLoadEvent[StartState]">
                                            <option value="" selected>Please Select</option>
                                            @foreach (var state in stateList)
                                            {
                                                if (sb.OnLoadEvent != null)
                                                {
