                            <div class="form-row">
                                <div class="col-lg-12">
                                    <p>Choose the Elements from which you want to take value from </p>
                                </div>
                            </div>
                            <div class="form-row">
                                <div class="col-lg-12">
                                    <div class="select-option-head">
                                        <div class="px-4">
                                            <div class="form-row">
                                                <div class="col-lg-4">
                                                    <label for="">Value Field</label>
                                                </div>
                                                <div class="col-lg-4">
                                                    <label for="">Select Element</label>
                                                </div>
                                                <div class="col-lg-3">
                                                    <label for="">Enable Validation</label>
                                                </div>
                                            </div>

                                        </div>
                                    </div>
                                    <div class="validation_rule_wrapper">
                                        @{
                                            if (sb.OnLoadEvent != null)
                                            {
                                                if (sb.OnLoadEvent.TakeValueFromName != null && sb.OnLoadEvent.TakeValueFromName.Count > 0)
                                                {

                                                    index1 = 0;
                                                    foreach (var so in sb.OnLoadEvent.TakeValueFromName)
                                                    {
                                                        <div class="field-validation-wrapper">
                                                            <div class="field-controller">
                                                                <a class="fc-icon icon-copy" data-action='clone-rule' title="Copy"><i class="ri-file-copy-line"></i></a>
                                                                <a class="fc-icon icon-delete" data-action='remove-rule' title="Delete"><i class="ri-delete-bin-4-line"></i></a>
                                                                @*<a class="fc-icon icon-timer" data-action='short-selectoption'><i class="ri-timer-2-line"></i></a>*@
                                                            </div>
                                                            <div class="form-row">
                                                                <div class="col-lg-4">
                                                                    <input type="text" name="OnLoadEvent[TakeValueFromName][@index1]" data-name="OnLoadEvent[TakeValueFromName][{0}]" class="form-control" id="" placeholder="Value Field" value="@sb.OnLoadEvent.TakeValueFromName[index1]">

                                                                </div>
                                                                <div class="col-lg-4">
                                                                    <select type="text" name="OnLoadEvent[TakeValueFromElement][@index1]" data-name="OnLoadEvent[TakeValueFromElement][{0}]" class="form-control" id="" placeholder="Text Field">
