                                        <select class="form-control endState" id="sbEndState" name="OnLoadEvent[EndState]">
                                            <option value="" selected>Please Select</option>
                                            @foreach (var state in stateList)
                                            {
                                                if (sb.OnLoadEvent != null)
                                                {
                                                    if (sb.OnLoadEvent.EndState == state.Value)
                                                    {
                                                        <option value="@state.Value" selected>@state.Text</option>
