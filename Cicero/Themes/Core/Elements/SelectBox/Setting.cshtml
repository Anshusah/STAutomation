                        <div class="switch-control">
                            <label for="sbVisibleinGrid" class="switch">
                                <input type="checkbox" id="sbVisibleinGrid" @sb.GetName("VisibleinGrid") class="switch__input" @sb.GetChecked(sb.VisibleinGrid) value="True">
                                <span class="switch__state"></span>
                                <span class="switch__label">Visible in Grid</span>
                            </label>
                        </div>

                    </div>
                </div>
                <div class="form-row">
                    <div class="form-group col-lg-6">
                        <label for="sbTableName">Table Name</label>
                        <select value="@sb.TableName" id="sbTableName" class="tableMap custom-select"></select>
                        <input type="text" value="@sb.TableName" @sb.GetName("TableName") hidden>
                    </div>
                    <div class="form-group col-lg-6">
                        <label for="sbFieldName">Field Name</label>
                        <select value="@sb.FieldName" id="sbFieldName" class="fieldMap custom-select"></select>
                        <input type="text" class="form-control" value="@sb.FieldName" @sb.GetName("FieldName") hidden>
                    </div>
                </div>

                <div class="form-group">
                    <div class="form-group col-lg-6">
                        <label for="SetValueFrom_@sb.ElementId">Set Value From</label>
                        <select value="@sb.SetValueFrom" id="SetValueFrom_@sb.ElementId" @sb.GetName("SetValueFrom") class="custom-select">
                            <option value="">Please Select</option>
                            @foreach (var item in formElements.Where(x => x.Value != sb.ElementId.ToString()))
                            {
                                <option selected="@(sb.SetValueFrom == item.Value)" value="@item.Value">@item.Text</option>
                            }
                        </select>
                    </div>
                </div>

            </section>
        </li>
        <li>
            <header>Automation Settings</header>
            <section>
                <p>Visible in automation?</p>
                <div class="switch-control">
                    <label for="sbAutomationEnable" class="switch">
                        <input class="switch__input" id="sbAutomationEnable" @sb.GetName("automationEnable") type="checkbox" value="true" @sb.GetChecked(sb.automationEnable)>
                        <span class="switch__state"></span>
                    </label>
                </div>
            </section>
        </li>
    </ul>
</div>
