                                                                        {
                                                                            if (sb.OnLoadEvent.TakeValueFromElement[index1] != null)
                                                                            {
                                                                                if (item.Value == sb.OnLoadEvent.TakeValueFromElement[index1].ToString())
                                                                                {
                                                                                    <option value="@item.Value" selected>@item.Text</option>
                                                                                }
