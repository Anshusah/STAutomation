                                        <div class="col-auto">
                                            <!-- <label class="btn btn-sm btn-invalid">Invalid</label> -->
                                            <span class="badge badge-danger p-2">Invalid</span>
                                        </div>
                                        <div class="col-auto">
                                            <label for="">if</label>
                                        </div>
                                        <div class="col-auto">
                                            @*<label class="muted-field"> @msb.BackendLabel</label>*@
                                            <input type="text" class="form-control" disabled value="@msb.BackendLabel">
                                        </div>
                                        <div class="col-lg-3">
                                            <select id="selectGroup" data-action="operator" name="Validations[@index1][FieldOperator]" data-name="Validations[{0}][FieldOperator]" class="custom-select">
                                                <option selected="@(val.FieldOperator=="required")" value="required">Required</option>
                                                <option selected="@(val.FieldOperator==">")" value=">">Greater Than</option>
                                                <option selected="@(val.FieldOperator=="=")" value="=">Equals To</option>
                                                <option selected="@(val.FieldOperator=="<")" value="<">Less Than</option>
                                                <option selected="@(val.FieldOperator=="between")" value="between">Between</option>

                                            </select>

                                        </div>
                                        <div class="col-lg-3" data-option="@val.ValidationValues?.FirstOrDefault()">
                                            @{
                                                if (val.FieldOperator == "required")
                                                {
                                                    <select class="custom-select" name="Validations[@index1][ValidationValues]">
                                                        @if (val.ValidationValues?.FirstOrDefault() == "true")
                                                        {
                                                            <option value="true" selected>True</option>
                                                        }
                                                        else
                                                        {
                                                            <option value="true">True</option>
                                                        }
                                                        @if (val.ValidationValues?.FirstOrDefault() == "false")
                                                        {
                                                            <option value="false" selected>False</option>
                                                        }
                                                        else
                                                        {
                                                            <option value="false">False</option>
                                                        }


                                                    </select>
                                                }
                                                else if (val.FieldOperator == "between")
                                                {
                                                    <input type="number" class="form-control" size="20" min="0" oninput="validity.valid||(value=value.replace(/\D+/g, ''))" value="@val.ValidationValues?.FirstOrDefault()" name="Validations[@index1][ValidationValues][0]" /><input type="number" min="1" size="20" class="form-control" value="@val.ValidationValues?.LastOrDefault()" name="Validations[@index1][ValidationValues][1]">
                                                }
                                                else if (val.FieldOperator == "<")
                                                {
                                                    <input type="number" class="form-control" size="20" min="0" oninput="validity.valid||(value=value.replace(/\D+/g, ''))" value="@val.ValidationValues?.FirstOrDefault()" name="Validations[@index1][ValidationValues]">
                                                }
                                                else if (val.FieldOperator == ">")
                                                {
                                                    <input type="number" class="form-control" size="20" min="0" oninput="validity.valid||(value=value.replace(/\D+/g, ''))" value="@val.ValidationValues?.FirstOrDefault()" name="Validations[@index1][ValidationValues]">

                                                }

                                                else
                                                {

                                                    <input type="number" class="form-control" size="20" min="0" oninput="validity.valid||(value=value.replace(/\D+/g, ''))" value="@val.ValidationValues?.FirstOrDefault()" name="Validations[@index1][ValidationValues]">

                                                }
                                            }
                                        </div>

                                    </div>
                                    <div class="form-row">
                                        <div class="col-lg-12">
                                            <textarea class="form-control " name="Validations[@index1][ErrorMessage]" data-name="Validations[{0}][ErrorMessage]" rows="2" id="" placeholder="Please provide valid @msb.FrontendLabel, @msb.FrontendLabel must be greater than 15 Number.">@val.ErrorMessage</textarea>
                                        </div>
                                    </div>
                                </div>
                                {
                                    index1++;
                                }
                            }

                        }
                        else
                        {
                            <div class="field-validation-wrapper">
                                <div class="field-controller">
                                    <a class="fc-icon icon-copy" data-action='clone-rule' title="Copy"><i class="ri-file-copy-line"></i></a>
                                    <a class="fc-icon icon-delete" data-action='remove-rule' title="Delete"><i class="ri-delete-bin-4-line"></i></a>
                                </div>
                                <div class="form-row">
                                    <div class="col-auto">
                                        <!-- <label class="btn btn-sm btn-invalid">Invalid</label> -->
                                        <span class="badge badge-danger p-2">Invalid</span>
                                    </div>
                                    <div class="col-auto ">
                                        <label for="">if</label>
                                    </div>
                                    <div class="col-auto">
                                        @*<label class="muted-field"> @msb.BackendLabel</label>*@
                                        <input type="text" class="form-control" disabled value="@msb.BackendLabel">
                                    </div>
                                    <div class="col-lg-3">
                                        <select id="selectGroup" data-action="operator" name="Validations[@index1][FieldOperator]" data-name="Validations[{0}][FieldOperator]" class="form-control custom">
                                            <option value="required" selected="selected">Required</option>
                                            <option value=">">Greater Than</option>
                                            <option value="=">Equals To</option>
                                            <option value="<">Less Than</option>
                                            <option value="between">Between</option>

                                        </select>

                                    </div>

                                    <div class="col-lg-3" data-option="">
                                        <select class="custom-select" name="Validations[@index1][ValidationValues]">
                                            <option value="true">True</option>
                                            <option value="false" selected="selected">False</option>
                                        </select>
                                    </div>

                                </div>
                                <div class="form-row">
                                    <div class="col-lg-12">
                                        <textarea class="form-control " name="Validations[0][ErrorMessage]" data-name="Validations[{0}][ErrorMessage]" rows="2" id="" placeholder="Please provide valid @msb.FrontendLabel, @msb.FrontendLabel must be greater than 15 Number."></textarea>
                                    </div>
                                </div>
                            </div>
                        }
                    }
                </div>
                <p class="small text-muted">This feature will let you add validation rule for this specific field.</p>

            </section>
        </li>
        <li>
            <header>Developer Options</header>
            <section>
                <div class="form-row">
                    <div class="form-group col-lg-6">
                        <label for="msbTemplate">Template</label>
                        <input type="text" class="form-control" id="msbTemplate" @msb.GetName("Template") value="@msb.Template" />
                    </div>
                    <div class="form-group col-lg-6">
                        <label for="msbWrapperTemplate">Wrapper Template</label>
                        <input type="text" class="form-control" id="msbWrapperTemplate" @msb.GetName("WrapperTemplate") value="@msb.WrapperTemplate" />
                    </div>
                </div>
                <div class="form-row">
                    <div class="form-group col-lg-4">
                        <div class="switch-control">
                            <label for="msbVisibleinGrid" class="switch">
                                <input type="checkbox" id="msbVisibleinGrid" @msb.GetName("VisibleinGrid") class="switch__input" @msb.GetChecked(msb.VisibleinGrid) value="True">
                                <span class="switch__state"></span>
                                <span class="switch__label">Visible in Grid</span>
                            </label>
                        </div>
                    </div>
                </div>
                <div class="form-row">
                    <div class="form-group col-lg-6">
                        <label for="msbTableName">Table Name</label>
                        <select value="@msb.TableName" id="msbTableName" class="tableMap custom-select"></select>
                        <input type="text" value="@msb.TableName" @msb.GetName("TableName") hidden>
                    </div>
                    <div class="form-group col-lg-6">
                        <label for="msbFieldName">Field Name</label>
                        <select value="@msb.FieldName" id="msbFieldName" class="fieldMap custom-select"></select>
                        <input type="text" class="form-control" value="@msb.FieldName" @msb.GetName("FieldName") hidden>
                    </div>
                </div>

            </section>
        </li>
        <li>
            <header>Automation Settings</header>
            <section>
                <p>Visible in automation?</p>
                <div class="switch-control">
                    <label for="msbAutomationEnable" class="switch">
                        <input class="switch__input" id="msbAutomationEnable" @msb.GetName("automationEnable") type="checkbox" value="true" @msb.GetChecked(msb.automationEnable)>
                        <span class="switch__state"></span>
                    </label>
                </div>
            </section>
        </li>
    </ul>
</div>
