<script>
        function animateValue(id, start, end, duration) {
            var range = start - end;
            var current = end;
            var increment = start > end? 1 : -1;
            var stepTime = Math.abs(Math.floor(duration / range));
            var obj = document.getElementById(id);
            var timer = setInterval(function() {
                current += increment;
                obj.innerHTML = current;
                if (current == start) {
                    clearInterval(timer);
                }
            }, stepTime);
        }
    
        animateValue("totalUsers", @Model.TotalUsers, 0, 500);
        animateValue("totalArticles", @Model.TotalArticles, 0, 500);
        animateValue("totalNewClaims", @Model.NewClaims, 0, 500);
        animateValue("totalClaims", @Model.TotalClaims, 0, 500);
    </script>

